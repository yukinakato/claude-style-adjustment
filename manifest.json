{
  "manifest_version": 3,
  "name": "claude-style-adjustment",
  "description": "Some style adjustment on Claude",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://claude.ai/*"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "icons": {
    "32": "icon_32.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "activeTab"
  ]
}